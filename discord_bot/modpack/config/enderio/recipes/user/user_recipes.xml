<enderio:recipes xmlns:enderio="http://enderio.com/recipes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://enderio.com/recipes recipes.xsd ">

<recipe name="Tweak: Stick from Wood" disabled="true">
    <crafting>
      <grid size="1x2">
        <item name="logWood"/>
        <item name="logWood"/>
      </grid>
      <output name="minecraft:stick, stickWood" amount="16" />
    </crafting>
  </recipe>
  
  <recipe name="Alloy: Neutral Steel, Variant: Tungsten, Vivid Alloy" required="false">
    <alloying energy="20000" exp="0.4">
      <input name="ingotTungsten"/>
	  <input name="contenttweaker:neutral_steel_4" amount="3"/>
	  <input name="ingotVividAlloy"/>
      <output name="contenttweaker:neutral_steel_5"/>
    </alloying>
  </recipe>
  
  <recipe name="Alloy: Electrical Steel" disabled="false">
	<alloying energy="10000" exp="0.5">
		<input name="ingotIron"/>
		<input name="dustCoal"/>
		<input name="itemSilicon"/>
		<output name="ingotElectricalSteel"/>
	</alloying>
 </recipe>
 
 <recipe name="Alloy: Electrical Steel, Variant" disabled="false">
	<alloying energy="10000" exp="0.4">
		<input name="ingotSteel"/>
		<input name="itemSilicon"/>
